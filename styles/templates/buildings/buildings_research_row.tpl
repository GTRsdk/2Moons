<tr>
	<th class="l" rowspan="2" width="120">
		<a href="javascript:infodiv({tech_id});javascript:animatedcollapse.toggle('{tech_id}')">
			<img border="0" src="{dpath}gebaeude/{tech_id}.gif" alt="{n}" align="top" width="120" height="120">
		</a>
	</th>
    <td class="c">
		<img src="./styles/images/transparent.gif" alt="" width="0" height="0">&nbsp;<a href="javascript:infodiv({tech_id});javascript:animatedcollapse.toggle('{tech_id}')">{tech_name}</a>{tech_level}</td>
	</td>
</tr>
<tr>
    <td colspan="1" class="l">
    	<table border="0" cellpadding="0" cellspacing="0">
    		<tbody>
				<tr>
					<td width="10px"><img src="./styles/images/transparent.gif" alt="" width="0" height="100"></td>
					<td width="90%">{tech_descr}<br><br>{tech_price}</td>
        			<td width="100px" style="text-align:center;vertical-align:middle;">
        			{tech_link}
					</td>
				</tr>
			</tbody>
		</table>
    </td>
</tr>
<tr>
    <td colspan="2" style="padding:0px;">  
        <table width="100%" border="0" cellpadding="0" cellspacing="0">
			<tbody>
				<tr>
					<td class="b">
						<table width="100%" align="center" cellpadding="2" cellspacing="0">
						<tbody>
						<tr>
							<td>
								<a class="b">{bd_remaining}</a><br><br>
							</td>
							<td colspan="2">
								<a class="b">{fgf_time}</a><br><br>
							</td>
						</tr>
						<tr>		
							<td width="68%" rowspan="3">

								{tech_restp}<br>{cancel}<br>
							</td>
							<td width="15%">
								<nobr>{search_time}</nobr>
							</td>
						</tr>				
						</tbody>
						</table>
					</td>
				</tr>
			</tbody>
		</table>
    </td>
</tr>
<tr>
    <td colspan="2"><img src="./styles/images/transparent.gif" alt="" width="1" height="10"></td>
</tr>
